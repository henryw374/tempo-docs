{
  "name": "tryclojure",
  "version": "1.0.0",
  "description": "Try Clojure in a web REPL!",
  "author": "Elia Scotto [@elias94]",
  "homepage": "https://github.com/elias94/tryclojure",
  "scripts": {
    "styles-watch": "npx tailwindcss -i src/styles/styles.css -o docs/public/css/styles.css --watch",
    "styles": "npx tailwindcss -i src/styles/styles.css -o docs/public/css/styles.css",
    "clean": "rm -rf /docs/public/js /docs/public/css",
    "build": "npm run clean &&  npm run styles"
  },
  "dependencies": {
    "node-libs-browser": "2.2.1",
    "platform": "1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "stack-trace": "0.0.10",
    "stacktrace-js": "2.0.2",
    "tailwindcss": "^3.0.23",
    "ws": "7.3.1"
  }
}
